<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GlobalStyleable">
        <attr name="title" format="string" />
        <attr name="text" format="string" />
        <attr name="hint" format="string" />
        <attr name="description" format="string" />
        <attr name="clickableViewEnabled" format="boolean" />
        <attr name="calendarViewEnabled" format="boolean" />
        <attr name="actionText" format="string" />
        <attr name="loading" format="boolean" />
        <attr name="endIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="title"/>
        <attr name="description"/>
        <attr name="hint"/>
        <attr name="clickableViewEnabled"/>
        <attr name="endIcon"/>
    </declare-styleable>
</resources>